<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows10.0.19041</TargetFramework>
    <UseWPF>true</UseWPF>
    <nullable>enable</nullable>
    <RootNamespace>ExternalNmeaGPS</RootNamespace>
    <SupportedOSPlatformVersion>10.0.14393</SupportedOSPlatformVersion>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Esri.ArcGISRuntime.WPF">
      <Version>100.12.0</Version>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Content Include="NmeaSampleData.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>